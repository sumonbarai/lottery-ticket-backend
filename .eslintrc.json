{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "quotes": "off",
    "no-else-return": "off",
    "consistent-return": "off",
    "no-plusplus": "off",
    "comma-dangle": "off",
    "no-unneeded-ternary": "off"
  }
}
